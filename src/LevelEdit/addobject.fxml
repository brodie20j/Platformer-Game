<GridPane fx:controller="LevelEdit.AddController"
          fx:id="mainBoard"
          xmlns:fx="http://javafx.com/fxml"
          alignment="center"
          hgap="10"
          vgap="10"
        >
    <Button fx:id = "addButton" GridPane.rowIndex="0" GridPane.columnIndex="0" text="Add Object" onAction="#addButton" />

    <Label text="Type" GridPane.rowIndex="1" GridPane.columnIndex="0" style="-fx-font: 10px Tahoma; "/>

    <ChoiceBox fx:id="typeInput" GridPane.rowIndex="1" GridPane.columnIndex="1">
    </ChoiceBox>
    <ChoiceBox fx:id="idInput" GridPane.rowIndex="1" GridPane.columnIndex="2">
    </ChoiceBox>
    <Label fx:id="constructorLabel" text="" GridPane.rowIndex="2"/>
    <GridPane fx:id="addBoard" GridPane.rowIndex="3"/>


</GridPane>