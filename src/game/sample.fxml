<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
        <?import javafx.scene.layout.Pane?>

<?import javafx.scene.shape.Rectangle?>
<AnchorPane
          xmlns:fx="http://javafx.com/fxml">
    <Pane fx:id="backgroundPane" layoutX="0" layoutY="0"></Pane>
    <Pane fx:id="gameView" layoutX="0" layoutY="0">

    <Pane fx:id="gameBoard" layoutX="0" layoutY="0">
        </Pane>
    </Pane>
    <AnchorPane fx:id="gui" AnchorPane.topAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.bottomAnchor="450"
                style="-fx-background-color: #FFFF66; -fx-border-color: #000; -fx-border-width:5px 2.5px 5px 2.5px; ">
        <Label fx:id="HealthLabel" layoutX="30" layoutY="10" style="-fx-text-color: #FFF"/>

        <Rectangle fx:id="HealthFill" layoutX="60" layoutY="12.5" style="-fx-background-color: #FFFF00; -fx-border-color: #000; -fx-border-width:5px 2.5px 5px 2.5px;"/>
        <Rectangle fx:id="HealthOutline" layoutX="60" layoutY="12.5" style="-fx-background-color: #FFFF00; -fx-border-color: #000; -fx-border-width:5px 2.5px 5px 2.5px;"/>

        <Label fx:id="CoinLabel" layoutX="300" layoutY="20" style="-fx-text-color: #FFF"/>
        <Label fx:id="txtLabel" layoutX="300" layoutY="10" style=" -fx-text-color: #000;"/>
        <Button fx:id="pauseButton" text="Pause" layoutX="250" AnchorPane.topAnchor="15" style="display: none"/>

    </AnchorPane>
</AnchorPane>